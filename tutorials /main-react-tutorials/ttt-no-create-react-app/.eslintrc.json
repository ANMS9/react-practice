{
  "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "rules": {
      "indent":[
        "error",
        2
      ],
      "semi": [
        "error",
        "always"
      ],
      "semi-spacing": [
        "error", { 
          "before": false, 
          "after": true
        }
      ],
      "key-spacing": [
        "error", {
          "beforeColon": false,
          "afterColon": true
        }

      ]
  }
}